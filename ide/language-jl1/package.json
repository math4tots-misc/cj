{
    "name": "language-jl1",
    "displayName": "language-jl1",
    "description": "jl1 language support",
    "version": "0.0.1",
    "publisher": "mtots",
    "repository": {
        "type": "git",
        "url": "https://github.com/math4tots/cj"
    },
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:jl1"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./"
    },
    "contributes": {
        "languages": [
            {
                "id": "jl1",
                "aliases": [
                    "jl1",
                    "jl1"
                ],
                "extensions": [
                    ".jl1"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jl1",
                "scopeName": "source.jl1",
                "path": "./syntaxes/jl1.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^12.12.0",
        "@typescript-eslint/eslint-plugin": "^3.0.2",
        "@typescript-eslint/parser": "^3.0.2",
        "eslint": "^7.1.0",
        "typescript": "^4.0.2",
        "@types/vscode": "^1.32.0"
    }
}
